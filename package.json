{
  "name": "map-object-ts",
  "version": "0.0.4",
  "description": "Typescript typesafe object mapper",
  "author": "aporovic",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aporovic/map-object-ts.git"
  },
  "bugs": {
    "url": "https://github.com/aporovic/map-object-ts/issues"
  },
  "keywords": [
    "typescript",
    "mapper",
    "copy same name properties",
    "typesafe"
  ],
  "type": "module",
  "private": false,
  "files": [
    "dist"
  ],
  "module": "./dist/map-object-ts.js",
  "main": "./dist/map-object-ts.js",
  "types": "./dist/mapObject.d.ts",
  "exports": {
    ".": {
      "import": "./dist/map-object-ts.js",
      "require": "./dist/map-object-ts.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "test": "vitest"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "eslint": "^9.28.0",
    "globals": "^16.2.0",
    "prettier": "^3.5.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.34.0",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.3"
  }
}
